
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <title>404 Error Template for Bootstrap</title>
    <!-- Bootstrap core CSS -->
    <link href="/templates/bootstrap-error-page/css/bootstrap.css" rel="stylesheet"><!-- Custom styles for this template -->
    <!-- Custom styles for this template -->
    <link href="/templates/bootstrap-error-page/css/style.css" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
</head>

<body>

    <div class="container">
        <div class="row">

            <div class="col-md-12">
                <h1><i class="fa fa-exclamation-triangle" aria-hidden="true"></i> 404</h1>
                <h2>Sayfa Bulunamadı</h2>
                <p>@ViewBag.Message</p>
                <h3>@ViewBag.Code</h3>
            </div>
        </div>
    </div>

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    ----------------------------------------------------------------
                </div>
            </div>
        </div>
    </footer>


    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="/templates/bootstrap-error-page/js/jquery.js"></script>
    <script src="/templates/bootstrap-error-page/js/bootstrap.js"></script>
</body>
</html>