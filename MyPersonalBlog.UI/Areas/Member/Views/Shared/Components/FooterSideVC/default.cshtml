@model AppUserListDto

<footer class="footer">
    <div class="container">
        <div class="row">
            <div class="col-lg-3 col-sm-3">
                <h1>Bağlantı Bilgileri</h1>
                <address>
                    <p>Telefon : @Model.PhoneNumber</p>
                    <p>Email : @Model.Email</p>
                </address>
            </div>
            <div class="col-lg-5 col-sm-5">

            </div>
            <div class="col-lg-3 col-sm-3 col-lg-offset-1">
                <h1>Bağlantıda Kalın</h1>
                <ul class="social-link-footer list-unstyled">
                    <li><a href="@Model.FacebookAddress" target="_blank"><i class="icon-facebook"></i></a></li>
                    <li><a href="@Model.LinkedInAddress" target="_blank"><i class="icon-linkedin"></i></a></li>
                    <li><a href="@Model.GitHubAddress" target="_blank"><i class="icon-github"></i></a></li>
                    <li><a href="@Model.YouTubeAddress" target="_blank"><i class="icon-youtube"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
</footer>